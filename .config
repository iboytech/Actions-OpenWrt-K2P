# 设置固件大小:
CONFIG_TARGET_ramips=y
CONFIG_TARGET_ramips_mt7621=y
CONFIG_TARGET_ramips_mt7621_DEVICE_phicomm_k2p=y
CONFIG_KERNEL_MIPS_FPU_EMULATOR=y

# 访客网络
CONFIG_PACKAGE_luci-app-guest-wifi=y

# 网络加速
CONFIG_PACKAGE_luci-app-turboacc=y
CONFIG_PACKAGE_luci-app-turboacc_INCLUDE_flow-offload=y
CONFIG_PACKAGE_luci-app-turboacc_INCLUDE_shortcut-fe=y

# 动态域名解析
CONFIG_PACKAGE_luci-app-ddns=y

# 文件传输
CONFIG_PACKAGE_luci-app-filetransfer=y

# 文件管理
CONFIG_PACKAGE_luci-app-fileassistant=y

# 多线多播
CONFIG_PACKAGE_luci-app-mwan3=y
CONFIG_PACKAGE_luci-app-syncdial=y

# 内网穿透
CONFIG_PACKAGE_luci-app-frpc=y
#CONFIG_PACKAGE_luci-i18n-frpc-zh-cn=m

# 流量统计
CONFIG_PACKAGE_luci-app-nlbwmon=n

# 访问控制
CONFIG_PACKAGE_luci-app-accesscontrol=y

# 其他需要安装的软件包:
# CONFIG_PACKAGE_frpc=y
CONFIG_PACKAGE_autocore-switch=y
CONFIG_PACKAGE_bash=n
CONFIG_PACKAGE_htop=n
CONFIG_PACKAGE_nano=n
CONFIG_PACKAGE_wget-ssl=n

# cifsmount
CONFIG_PACKAGE_kmod-fs-cifs=n
CONFIG_PACKAGE_kmod-nls-utf8=n
CONFIG_PACKAGE_kmod-crypto-misc=n

# ignore
CONFIG_PACKAGE_kmod-wireguard=n
CONFIG_PACKAGE_luci-app-upnp=n
CONFIG_PACKAGE_luci-app-arpbind=n
# KMS
CONFIG_PACKAGE_luci-app-vlmcsd=n
# FTP
CONFIG_PACKAGE_luci-app-vsftpd=n
# 远程唤醒
CONFIG_PACKAGE_luci-app-wol=n
# 释放内存
CONFIG_PACKAGE_luci-app-ramfree=n
CONFIG_PACKAGE_luci-app-ssr-plus=n
# 网络诊断
CONFIG_PACKAGE_luci-app-diag-core=n


